{
  "name": "@packlint/sort",
  "version": "0.0.6",
  "exports": {
    ".": {
      "require": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "exports": {
      ".": {
        "require": "./dist/index.js",
        "import": "./esm/index.mjs",
        "types": "./dist/index.d.ts"
      },
      "./package.json": "./package.json"
    }
  },
  "main": "./src/index.ts",
  "scripts": {
    "prepack": "yarn build",
    "test": "jest",
    "build": "rm -rf dist esm && tsc --declaration --emitDeclarationOnly --declarationDir dist && rollup -c rollup.config.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@packlint/command": "workspace:^",
    "@packlint/core": "workspace:^",
    "clipanion": "^3.2.0-rc.13",
    "fast-sort": "^3.2.0",
    "ts-pattern": "4.0.5",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@toss/rollup-config": "^0.0.1",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "fast-check": "^3.3.0",
    "jest": "^29.2.2",
    "rollup": "2.79.1",
    "typescript": "4.8.3",
    "zod-fast-check": "^0.8.0"
  }
}
