{
  "name": "@packlint/core",
  "version": "0.0.2",
  "exports": {
    ".": {
      "require": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "exports": {
      ".": {
        "require": "./dist/index.js",
        "import": "./esm/index.mjs",
        "types": "./dist/index.d.ts"
      },
      "./package.json": "./package.json"
    }
  },
  "main": "./src/index.ts",
  "scripts": {
    "prepack": "yarn build",
    "test": "jest",
    "build": "rm -rf dist esm && tsc --declaration --emitDeclarationOnly --declarationDir dist && rollup -c rollup.config.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "cosmiconfig": "^7.0.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@toss/rollup-config": "^0.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.1",
    "jest": "^29.2.2",
    "rollup": "2.79.1",
    "typescript": "4.8.3"
  }
}
