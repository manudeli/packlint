{
  "name": "packlint",
  "version": "0.0.8",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "bin": "./bin/index.js",
  "scripts": {
    "prepack": "yarn build",
    "build": "rm -rf dist && tsup ./src/index.ts --format esm --dts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@packlint/command": "workspace:^",
    "@packlint/core": "workspace:^",
    "clipanion": "^3.2.0-rc.13"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "tsup": "^6.5.0",
    "typescript": "4.8.3",
    "vite": "^4.0.2",
    "vitest": "^0.26.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
